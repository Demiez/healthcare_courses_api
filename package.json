{
  "name": "healthcare-courses-api",
  "version": "1.0",
  "description": "HEALTHCARE COURSES API",
  "main": "./dist/server.js",
  "scripts": {
    "build": "yarn clean && tsc",
    "clean": "rimraf dist && rimraf reports",
    "lint": "tslint ./src/**/*.ts ./test/**/*.spec.ts",
    "lint:fix": "tslint --fix ./src/**/*.ts ./test/**/*.spec.ts -t verbose",
    "pretest": "cross-env NODE_ENV=test yarn build && yarn lint",
    "test": "cross-env NODE_ENV=test mocha --reporter spec --require ts-node/register test/**/*.spec.ts ",
    "test:mutation": "stryker run",
    "stryker:init": "stryker init",
    "dev": "nodemon",
    "prod": "yarn build && yarn start",
    "tsc": "tsc"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "keywords": [
    "HEALTHCARE COURSES API"
  ],
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "chai-http": "^4.3.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "swagger-express-ts": "^1.1.0",
    "swagger-ui-dist": "^4.0.0-beta.2"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^4.1.2",
    "@stryker-mutator/mocha-runner": "^4.1.2",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.14",
    "@types/chai-http": "^3.0.5",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.9",
    "@types/lodash": "^4.14.172",
    "@types/mocha": "^8.0.4",
    "@types/node": "^14.14.9",
    "@types/swagger-ui-dist": "^3.30.1",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.12",
    "rimraf": "^3.0.2",
    "ts-node": "^9.0.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.1.2"
  }
}
